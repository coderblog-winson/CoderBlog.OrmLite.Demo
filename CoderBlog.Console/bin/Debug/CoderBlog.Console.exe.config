<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
 <connectionStrings>
    <add name="Default" connectionString="Data Source=.;Initial Catalog=CoderBlog;User Id=sa;Password=master;Persist Security Info=False" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <!--Update all tables-->
    <add key="UpdateAll" value="true" />
    <!--What's tables need to be update, split with comma and just will do when UpdateAll is false-->
    <add key="UpdateTables" value="Page" />
    <!--Code first Model for generate the tables in database-->
    <add key="ModelNamespace" value="CoderBlog.Model" />
  </appSettings>
</configuration>